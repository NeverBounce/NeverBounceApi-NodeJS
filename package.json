{
  "name": "neverbounce",
  "version": "5.0.0",
  "description": "An API wrapper for the NeverBounce API",
  "engines": {
    "node": ">=20.0.0"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/NeverBounce/NeverBounceApi-NodeJS"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepublishOnly": "npm run build",
    "example": "tsx"
  },
  "keywords": [
    "email",
    "email verification",
    "email validation",
    "email cleaning",
    "verification",
    "validation",
    "cleaning"
  ],
  "author": "NeverBounce <support@neverbounce.com> (https://neverbounce.com)",
  "contributors": [
    "Mike Mollick <mike@neverbounce.com>",
    "Omri Katz <omri.katz@zoominfo.com>"
  ],
  "homepage": "https://neverbounce.com",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.17.41",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "chai": "^4.3.10",
    "eslint": "^8.56.0",
    "nock": "^13.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "dotenv": "^16.5.0"
  }
}
